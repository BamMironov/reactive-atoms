{
  "compilerOptions": {
    "target": "ESnext",
    "module": "ESNext",
    "lib": ["esnext"],
    "importHelpers": true,
    "moduleResolution": "NodeNext",
    "declaration": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./dist/lib/es6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
